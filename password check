

#include <iostream>
using namespace std;

int main() {
    string password;
    string username;
    string user_username;
    string user_password;
    string make_password;
    
    //registration
    cout<<"Enter username you want to save:";
    cin>>username;
    cout<<"Enter password you want to save:";
    cin>>make_password;
    //login
    cout<<"Enter your username:";
    cin>>user_username;
    cout<<"Enter your password:";
    cin>>password;
    
    //checking
    if( username==user_username&&make_password==password){
        cout<<"Login sucessful";
        
    }
    else{
        cout<<"Failed Login,username or password incorrect";
    }
    
    
    
    
    return 0;
}
